# Install GNU `sed`, overwriting the built-in `sed`.
brew install gnu-sed --with-default-names
# Install GNU `find`, `locate`, `updatedb`, and `xargs`, `g`-prefixed.
brew_install_or_upgrade 'findutils'
brew_install_or_upgrade 'moreutils'
brew_install_or_upgrade 'curl'
brew_install_or_upgrade 'gpg'
brew_install_or_upgrade 'ssh-copy-id'
brew_install_or_upgrade 'pv'
brew_install_or_upgrade 'trash'
brew_install_or_upgrade 'p7zip'
brew_install_or_upgrade 'fasd'
brew_install_or_upgrade 'python'
brew_install_or_upgrade 'go'
brew_install_or_upgrade 'boot2docker'
brew_install_or_upgrade 'docker'
brew_install_or_upgrade 'packer'
brew_install_or_upgrade 'dnsmasq'
brew_install_or_upgrade 'rbenv-gem-rehash'
brew_install_or_upgrade 'rbenv-default-gems'
brew_install_or_upgrade 'cowsay'
